<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<!-- Criado em 18/11/2019       -->
<hibernate-mapping package="br.gov.jfrj.siga.dp" schema="CORPORATIVO">
	<class name="CpContrato" mutable="false"
		table="CP_CONTRATO" schema="CORPORATIVO"> <!--  schema="CORPORATIVO" -->
		<id name="idOrgaoUsu" column="ID_ORGAO_USU" type="long" />
	    <property name="dtContrato" column="DT_CONTRATO"
		    type="java.util.Date"/>
	</class>
</hibernate-mapping>